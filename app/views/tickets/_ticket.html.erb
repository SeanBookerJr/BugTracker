<div id="<%= dom_id ticket %>">
  <p>
    <strong>Title:</strong>
    <%= ticket.title %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= ticket.type %>
  </p>

  <p>
    <strong>Priority:</strong>
    <%= ticket.priority %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= ticket.description %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= ticket.status %>
  </p>

  <p>
    <strong>User:</strong>
    <%= ticket.user_id %>
  </p>

  <p>
    <strong>Developer:</strong>
    <%= ticket.developer_id %>
  </p>

  <p>
    <strong>Manager:</strong>
    <%= ticket.manager_id %>
  </p>

  <p>
    <strong>Admin:</strong>
    <%= ticket.admin_id %>
  </p>

  <p>
    <strong>Project:</strong>
    <%= ticket.project_id %>
  </p>

</div>
