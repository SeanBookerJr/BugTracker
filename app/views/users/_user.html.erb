<div id="<%= dom_id user %>">
  <p>
    <strong>Username:</strong>
    <%= user.username %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= user.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Developer:</strong>
    <%= user.developer_id %>
  </p>

  <p>
    <strong>Manager:</strong>
    <%= user.manager_id %>
  </p>

  <p>
    <strong>Admin:</strong>
    <%= user.admin_id %>
  </p>

</div>
